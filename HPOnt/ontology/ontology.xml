        <rdfs:label xml:lang="en">Single Phase Power Supply Heat Pump</rdfs:label>
        <ns1:term_status rdf:datatype="http://www.w3.org/2001/XMLSchema#string">testing</ns1:term_status>
    </owl:Class>
    


