    <!-- https://w3id.org/hpont#HeatPump -->


    <owl:Class rdf:about="https://w3id.org/hpont#HeatPump">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/hpont#SinglePhasePowerSupplyHeatPump"/>
                    <rdf:Description rdf:about="https://w3id.org/hpont#ThreePhasePowerSupplyHeatPump"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">A device that transfers heat from a colder area to a hotter area by using mechanical energy</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/hpont#"/>
        <rdfs:label xml:lang="en">Heat Pump</rdfs:label>
        <ns1:term_status rdf:datatype="http://www.w3.org/2001/XMLSchema#string">stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/hpont#SinglePhasePowerSupplyHeatPump -->


